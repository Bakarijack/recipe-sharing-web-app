{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
      ],
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "rules": {
        "camelcase": "error",
        "spaced-comment": "error",
        "quotes": ["error", "single"],
        "no-duplicate-imports": "error",
        "arrow-body-style": ["error", "as-needed"]
    }
  }
  